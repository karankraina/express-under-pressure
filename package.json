{
  "name": "express-under-pressure",
  "version": "0.4.3",
  "description": "Express Under Pressure",
  "main": "lib/index.js",
  "type": "commonjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "tsc --noEmit && node --import tsx --test test/**/*test.ts",
    "test:coverage": "tsc --noEmit && node --import tsx --test --experimental-test-coverage test/**/*test.ts",
    "build": "tsc",
    "linter:fix": "eslint . --fix --ext .ts",
    "linter": "eslint . --ext .ts",
    "format": "prettier --write .",
    "lint": "npm run format && npm run linter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karankraina/express-under-pressure.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "under",
    "pressure"
  ],
  "author": "Karan Raina <karanraina1996@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karankraina/express-under-pressure/issues"
  },
  "homepage": "https://github.com/karankraina/express-under-pressure#readme",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.13.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^5.2.1",
    "prettier": "^3.3.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5"
  }
}
